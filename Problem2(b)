#include <iostream>

using namespace std;

int main()
{
    double num1, num2;
    cout << "Enter first number" << endl;
    cin >> num1;
    cout << "Enter second number" << endl;
    cin >> num2;
    if(num1>num2){
        cout <<"large number is: "<< num1;
    } else if(num1==num2) {
        cout <<"Both are equal ";
    } else{
        cout <<"large number is: "<< num2;
    }
    return 0;
}
